<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <!-- FORM VIEW: -->
    <record id="product_tag_shoes_dealer_inh_form" model="ir.ui.view">
        <field name="name">product.tag.shoes.dealer.inh.form</field>
        <field name="model">product.tag</field>
        <field name="arch" type="xml">
            <xpath expr="//group" position="after">
                <group>
                    <field name="pricelist_print"/>
                </group>
                <group attrs="{'invisible':[('pricelist_print','=',False)]}">
                    <group>
                        <field name="pricelist_image_type" widget="image" class="oe_avatar"/>
                    </group>
                    <group>
                        <field name="pricelist_image_feature" widget="image" class="oe_avatar"/>
                    </group>
                </group>
            </xpath>
        </field>
        <field name="inherit_id" ref="product.product_tag_form_view"/>
        <field name="key" eval="False"/>
        <field name="mode">extension</field>
        <field name="priority">66</field>
        <field name="type">form</field>
    </record>

</odoo>
