<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    # La tarifa no se puede modificar, es la asignada al partner o direcci√≥n de entrega:
    <record id="sale_order_custom_inplast" model="ir.ui.view">
        <field name="name">Sale Order Custom Inplast</field>
        <field name="model">sale.order</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='order_details']/div[3]/field[@name='pricelist_id']" position="attributes">
                <attribute name="attrs">{'readonly':True}</attribute>
            </xpath>
        </field>
    </record>

</odoo>
